<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <TextView
        android:id="@id/textView2"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10.0dip"
        android:gravity="center_horizontal"
        android:text="PRINCIPAL CONFIDENTIAL REPORTS"
        android:textAllCaps="false"
        fancy:layout_constraintLeft_toLeftOf="parent"
        fancy:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@id/textView5"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10.0dip"
        android:gravity="center_horizontal"
        android:text="REPORTS GENERATED REAL TIME "
        fancy:layout_constraintLeft_toLeftOf="parent"
        fancy:layout_constraintRight_toRightOf="parent" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNtotalclassenrollment"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:onClick="totals_per_class"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="Total Enrollment per class"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNtotalstreamenrolment"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:onClick="totals_per_stream"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="TOTAL ENROLMENT PER STREAM"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNtotalschoolenrolment"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:onClick="total_for_school"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="TOTAL SCHOOL ENROLMENT"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNstreamexamperformance"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:onClick="totalclassperformance"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="EXAM PERFORMANCE PER STREAM"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNbest10"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:onClick="best10students"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="BEST 10 STUDENTS PER STREAM"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTN10last"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:onClick="last10students"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="LAST 10 STUDENTS PER STREAM"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNtla"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text=" %TEACHER LESSON ATTENDANCE"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNsla"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="% STUDENT LESSON ATTENDANCE"
        fancy:fb_textColor="#ffffffff" />
</LinearLayout>